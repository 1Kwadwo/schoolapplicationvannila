{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "/frontend/css/$1",
      "headers": {
        "Accept-Ranges": "bytes",
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/js/(.*)",
      "dest": "/frontend/js/$1",
      "headers": {
        "Accept-Ranges": "bytes",
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js",
      "headers": {
        "Accept-Ranges": "none"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
